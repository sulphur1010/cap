-#.page
	- #if @page
		= #render :partial => 'content_fragments/page', :object => @page
	- #else
		-#Create a page with a url of "/about_us/team" to not see this page anymore.
.page
	%h2 CAPP-USA Team
.page.national_board_member_content
	%h3 National Board Members
	= render :partial => 'users/board_member', :collection => User.national_board_members.order(:about_us_weight)
.page.national_board_member_content
	%h3 Staff
	= render :partial => 'users/board_member', :collection => User.staff.order(:about_us_weight)
.page.national_board_member_content
	%h3 Chapter Presidents
	= render :partial => 'users/board_member', :collection => User.chapter_presidents.order(:about_us_weight)
